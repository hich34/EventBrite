<br><br>
<div class="card m-5">
  <h5 class="card-header"><%= @user.first_name %> <%= @user.last_name %></h5>

  <div class="card-body">
    <p class="card-text"><%= @user.description %></p>
    <p class="card-text"><%= @user.email %></p>

   <!--  <a href="#" class="btn btn-primary">Go somewhere</a> -->
  </div>
</div>
<h3 class="text-center"> Event Created </h3>
<% Event.all.each do |event| %>
<% if event.admin_id == @user.id %>
	<div class="container">
	<div class="card" style="width: 18rem;">
	 <div class="card-body">
	   <h5 class="card-title"><%= event.title %></h5>
	   <h6 class="card-subtitle mb-2 text-muted"><%= event.price %> balles</h6>
	   <p class="card-text"><%=event.description%></p>
	   <h6 class="card-subtitle mb-2 text-muted">City: <%= event.location %></h6>
	 </div>
	 </div>
	</div><br>
	<% end %>
	<% end %>